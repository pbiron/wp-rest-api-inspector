{
	"name" : "shc/wp-rest-api-inspector",
	"description" : "Inspect WordPress REST API routes, endpoints, parameters and properties",
	"type" : "wordpress-plugin",
	"license" : "GPL-2.0-or-later",
	"minimum-stability": "dev",
	"config": {
		"preferred-install": {
			"shc/*": "dist"
		}
	},
	"repositories": {
		"shc-framework": {
			"type": "vcs",
			"url": "git@github.com:pbiron/shc-framework.git"
		}
	},
	"autoload" : {
		"classmap" : ["plugin.php", "includes", "vendor/shc"],
		"exclude-from-classmap": ["components/"]
	},
	"require": {
		"shc/shc-framework": "^0.1.5"
	}
}
